<?xml version="1.0" ?>

<robot name="alphabot2" xmlns:xacro="https://www.ros.org/wiki/xacro" >
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.203125 0.23828125 0.28515625 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
  </material>
  <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="brown">
    <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0"/>
  </material>
  <material name="red">
    <color rgba="0.80078125 0.12890625 0.1328125 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>

  <gazebo reference="link_chassis">
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="link_left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="link_right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- chassis -->
  <link name="link_chassis">
    <!-- pose and inertial -->
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.145"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    <!-- end of pose and inertial -->
    
    <!-- chassis collision and visual -->
    <collision name="collision_chassis">
      <geometry>
        <cylinder radius="0.055" length="0.045"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.055" length="0.045"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <!-- end of chassis collision and visual -->
    
    <!-- caster front collision and visual -->
    <collision name="caster_front_collision">
      <origin rpy="0 0 0" xyz="0.05 0 -0.0335"/>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="caster_front_visual">
      <origin rpy="0 0 0" xyz="0.05 0 -0.0335"/>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <material name="white"/>
    </visual>
    <!-- end of caster front collision and visual -->

    <!-- caster back collision and visual -->
    <collision name="caster_back_collision">
      <origin rpy="0 0 0" xyz="-0.05 0 -0.0335"/>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual name="caster_back_visual">
      <origin rpy="0 0 0" xyz="-0.05 0 -0.0335"/>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <material name="white"/>
    </visual>
    <!-- end of caster back collision and visual -->
  </link>
  <!-- end of chassis -->

  <!-- right wheel -->
  <link name="link_right_wheel">
    <inertial>
      <mass value="0.025"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>
    
    <collision name="link_right_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.017" radius="0.021"/>
      </geometry>
    </collision>
    
    <visual name="link_right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.017" radius="0.021"/>
      </geometry>      
      <material name="black"/>
    </visual>
  </link>
  <!-- end of right wheel -->

  <!-- joint for right wheel -->
  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.046 -0.016"/>
    <child link="link_right_wheel" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>
  <!-- end of joint for right wheel -->

  <!-- left wheel -->
  <link name="link_left_wheel">
    <inertial>
      <mass value="0.025"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.00052666666" ixy="0" ixz="0" iyy="0.00052666666" iyz="0" izz="0.001"/>
    </inertial>

    <collision name="link_left_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.017" radius="0.021"/>
      </geometry>
    </collision>
    
    <visual name="link_left_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.017" radius="0.021"/>
      </geometry>      
      <material name="black"/>
    </visual>
  </link>
  <!-- end of left wheel -->

  <!-- joint for left wheel -->
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.046 -0.016"/>
    <child link="link_left_wheel" />
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0" />
  </joint>
  <!-- end of joint for left wheel -->

</robot>